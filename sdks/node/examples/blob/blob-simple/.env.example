# Temps Blob Configuration
# Copy this file to .env and fill in your values

# Your Temps API URL
TEMPS_API_URL=https://app.temps.kfs.es

# Your API key or deployment token
TEMPS_TOKEN=your-api-key-here

# Project ID (required when using API keys, optional for deployment tokens)
# Deployment tokens have project_id embedded, API keys need to specify it
TEMPS_PROJECT_ID=1
