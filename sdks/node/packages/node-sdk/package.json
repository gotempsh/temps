{
  "name": "@temps-sdk/node-sdk",
  "version": "0.0.1-alpha.4",
  "private": false,
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "build": "tsc -p tsconfig.build.json",
    "generate": "npx @hey-api/openapi-ts -i openapi.json -o src/client -c @hey-api/client-fetch"
  },
  "devDependencies": {
    "@hey-api/openapi-ts": "^0.82.4",
    "@types/bun": "latest",
    "@vitest/coverage-v8": "^3.2.4",
    "vitest": "^3.2.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@hey-api/client-fetch": "^0.13.1"
  }
}
